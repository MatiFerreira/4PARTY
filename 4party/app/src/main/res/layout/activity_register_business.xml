<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent"
        android:background="#212121"
        tools:context=".LogInOutBusiness.RegisterBusiness">


    <TextView
            android:text="@string/tituloDatosPersonal"
            android:textColor="#442B49"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="25dp"
            android:textAlignment="center"
            android:id="@+id/textView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"
            app:layout_constraintHorizontal_bias="0.0"
    />
    <EditText android:layout_width="match_parent"
              android:layout_height="50dp"
              android:hint="@string/dniautonomo"
              android:paddingLeft="30dp"
              android:background="@drawable/textviewredondo"
              android:textColorHint="#442B49"
              android:textColor="#442B49"
              android:layout_marginTop="12dp"
              app:layout_constraintTop_toBottomOf="@+id/textView"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintHorizontal_bias="0.0"
              android:id="@+id/dniAutonomoET"/>
    <EditText android:layout_width="match_parent"
              android:layout_height="50dp"
              android:id="@+id/nombreAutonomoET"
              android:textColor="#442B49"
              android:hint="@string/nombreautono"
              android:background="@drawable/textviewredondo"
              android:paddingLeft="30dp"
              android:textColorHint="#442B49"
              app:layout_constraintTop_toBottomOf="@+id/dniAutonomoET"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintEnd_toEndOf="parent"
              android:layout_marginTop="8dp"
              app:layout_constraintHorizontal_bias="0.0"/>

    <EditText android:layout_width="match_parent"
              android:layout_height="50dp"
              android:id="@+id/apellidoAutonomoET"
              android:textColor="#442B49"
              android:background="@drawable/textviewredondo"
              android:hint="@string/apellidoautono"
              android:textColorHint="#442B49"
              android:paddingLeft="30dp"
              app:layout_constraintTop_toBottomOf="@+id/nombreAutonomoET"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintEnd_toEndOf="parent"
              android:layout_marginTop="8dp"
              app:layout_constraintHorizontal_bias="0.0"/>

    <EditText android:layout_width="match_parent"
              android:layout_height="50dp"
              android:layout_marginTop="8dp"
              android:id="@+id/codigoPostalAutonomoET"
              android:textColor="#442B49"
              android:hint="@string/codigopostalautono"
              android:textColorHint="#442B49"
              android:paddingLeft="30dp"
              android:background="@drawable/textviewredondo"
              app:layout_constraintTop_toBottomOf="@+id/apellidoAutonomoET"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintEnd_toEndOf="parent"/>

    <EditText android:layout_width="match_parent"
              android:layout_height="50dp"
              android:hint="@string/nameEstablecimientoautonomo"
              android:background="@drawable/textviewredondo"
              android:paddingLeft="30dp"
              android:textColor="#442B49"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintEnd_toEndOf="parent"
              android:id="@+id/nameEstablecimientoET"
              android:textColorHint="#442B49"
              app:layout_constraintHorizontal_bias="0.0"
              app:layout_constraintTop_toBottomOf="@+id/codigoPostalAutonomoET"
              android:layout_marginTop="8dp"/>

    <TextView android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:text="@string/titulo2creacioncorreo"
              android:textSize="25sp"
              android:textAlignment="center"
              android:textColor="#442B49"
              app:layout_constraintTop_toBottomOf="@+id/nameEstablecimientoET"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintHorizontal_bias="0.0"
              android:id="@+id/textview"
              android:layout_marginTop="4dp"/>

    <EditText android:layout_width="match_parent"
              android:layout_height="50dp"
              app:layout_constraintTop_toBottomOf="@+id/correoAutonomoET"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintEnd_toEndOf="parent"
              android:layout_marginTop="8dp"
              android:inputType="textPassword"
              android:background="@drawable/textviewredondo"
              android:hint="@string/contrasenia"
              android:textColor="#442B49"
              android:paddingLeft="30dp"
              android:textColorHint="#442B49"
              app:layout_constraintHorizontal_bias="0.0"
              android:id="@+id/contraseniaAutonomoET"/>

    <EditText android:layout_width="match_parent"
              android:layout_height="50dp"
              app:layout_constraintTop_toBottomOf="@+id/textview"
              android:layout_marginTop="20dp"
              android:hint="@string/correoautono"
              android:inputType="textEmailAddress"
              android:textColorHint="#442B49"
              android:paddingLeft="30dp"
              android:textColor="#442B49"
              android:background="@drawable/textviewredondo"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintHorizontal_bias="0.0"
              android:id="@+id/correoAutonomoET"/>

    <EditText
        android:id="@+id/contrasenia2AutonomoET"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/textviewredondo"
        android:hint="@string/contrasenia"
        android:inputType="textPassword"
        android:paddingLeft="30dp"
        android:textColor="#442B49"
        android:textColorHint="#442B49"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contraseniaAutonomoET" />

    <Button
        android:id="@+id/crearcuentaAutonomoBTN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:background="@drawable/botonredondo"
        android:text="@string/botonregister"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.759"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contrasenia2AutonomoET" />

    <Button
        android:id="@+id/cancellautonomobutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:background="@drawable/botonredondo"
        android:text="Cancell"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.253"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contrasenia2AutonomoET" />


</androidx.constraintlayout.widget.ConstraintLayout>
