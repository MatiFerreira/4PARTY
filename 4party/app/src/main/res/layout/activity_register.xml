<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/black"
              android:orientation="vertical"
              tools:context=".LogInOutUser.Register_activity">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="411dp"
        android:layout_height="34dp"
        android:layout_marginTop="1dp"
        android:text="Nombre:"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_logo"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/BTNcancelar"
        android:layout_width="132dp"
        android:layout_height="43dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="42dp"
        android:background="@drawable/botonredondo"
        android:text="Cancelar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contrasenaEditText2"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/contrasenaEditText2"
        android:layout_width="331dp"
        android:layout_height="24dp"
        android:layout_marginTop="13dp"
        android:background="@color/white"
        android:hint="Repetir contraseña"
        android:inputType="textPassword"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contrasenaEditText"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/BTNregistrarse"
        android:layout_width="121dp"
        android:layout_height="41dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="42dp"
        android:background="@drawable/botonredondo"
        android:text="Registrarse"
        app:layout_constraintStart_toEndOf="@+id/BTNcancelar"
        app:layout_constraintTop_toBottomOf="@+id/contrasenaEditText2"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/contrasenaEditText"
        android:layout_width="331dp"
        android:layout_height="24dp"
        android:background="@color/white"
        android:hint="Contraseña"
        android:inputType="textPassword"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        android:text="Contraseña"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/correoElectronicoEditText"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Correo electronico"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dniEditText"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/dniEditText"
        android:layout_width="331dp"
        android:layout_height="24dp"
        android:background="@color/white"
        android:hint="14322332Q"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="331dp"
        android:layout_height="24dp"
        android:layout_marginTop="34dp"
        android:text="DNI"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/apellidosEditText"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Apellidos"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nombreEditText"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/apellidosEditText"
        android:layout_width="331dp"
        android:layout_height="24dp"
        android:layout_marginTop="12dp"
        android:background="@color/white"
        android:hint="ej: Ruiz Sanchez"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/nombreEditText"
        android:layout_width="331dp"
        android:layout_height="24dp"
        android:layout_marginTop="2dp"
        android:background="@color/white"
        android:hint="ej: Miguel"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/correoElectronicoEditText"
        android:layout_width="331dp"
        android:layout_height="24dp"
        android:layout_marginTop="8dp"
        android:background="@color/white"
        android:hint="ej: 4party@gmail.com"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView_logo"
        android:layout_width="125dp"
        android:layout_height="180dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo4part"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>
